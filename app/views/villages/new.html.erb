<% provide(:title, '村を作成') %>
<h3>村を作成</h3>
<div style="text-align: center;">
  <%= form_for(@village) do |f| %>
  <div class="input-field">
    <%= f.text_field :name, autofocus: true %>
    <%= f.label :name %>
  </div>
  <div class="input-field">
    <%= f.password_field :password, autofocus: true%>
    <%= f.label :password %>
  </div>
  <%= f.submit "作成", class: "waves-effect waves-light btn-large" %>
<% end %>
</div>